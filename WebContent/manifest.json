{
	"_version": "1.1.0",
	"sap.app": 
	{
		"_version": "1.1.0",
		"id": "springEmployee",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "",
		"description": "",
		"applicationVersion": 
		{
			"version": "1.0.0"
		},

		"ach": "CA-UI5-DOC",
		"dataSources": 
		{
			"invoiceRemote": 
			{
				"uri": "/proxy/http/services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": 
				{
					"odataVersion": "2.0"
				}
			}
		}
	},

	"sap.ui": 
	{
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": 
		{
			"desktop": true,
			"tablet": true,
			"phone": true
		},

		"supportedThemes": 
		[
			"sap_bluecrystal"
		]
	},

	"sap.ui5": 
	{
		"_version": "1.1.0",
		"rootView": "ems.view.App",
		"dependencies": 
		{
			"minUI5Version": "1.30",
			"libs": 
			{
				"sap.m": 
				{
					
				}
			}
		},

		"models": 
		{
			"i18n": 
			{
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": 
				{
					"bundleName": "ems.i18n.i18n"
				}
			},

			"invoice": 
			{
				"dataSource": "invoiceRemote"
			}
		},

		"resources": 
		{
			"css": 
			[
				{
					
				}
			]
		},

		"routing": 
		{
			"config": 
			{
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ems.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},

			"routes": 
			[
				{
					"pattern": "",
					"name": "EmployeeList",
					"target": "EmployeeList"
				},
				
				{
					"pattern": "EmployeeDetail/{id}",
					"name": "EmployeeDetail",
					"target": "EmployeeDetail"
				}
			],

			"targets": 
			{
				"EmployeeList":
				{
					"viewName": "EmployeeList"
				},
				
				"EmployeeDetail":
				{
					"viewName": "EmployeeDetail"
				}
			}
		}
	}
}